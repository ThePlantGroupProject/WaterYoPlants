from bs4 import BeautifulSoup
from urllib.request import Request, urlopen
import re

fhandle=open('reference.txt', 'w' , encoding= 'utf-8')
## Opens Pre-made txt files with links to the 
with open("links.txt") as file:
        for line in file:
            urls = line.strip()
            #print(urls)
            req = Request(urls)
            html_page = urlopen(req)
            soup = BeautifulSoup(html_page, "html.parser")
            html_text = soup.get_text()
            fhandle.write("\n {} ".format(html_text))
fhandle.close()  
